{
  "name": "digital-twin",
  "version": "2.6.67",
  "description": "## Background",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"pnpm run simulator\" \"pnpm run visualisation\"",
    "dev": "pnpm run simulator",
    "simulator": "pnpm --filter @digital-twin/simulator dev",
    "visualisation": "pnpm --filter visualisation dev",
    "build": "pnpm --recursive run build --if-present",
    "lint": "pnpm --recursive --if-present run lint"
  },
  "repository": {
