  "bugs": {
    "url": "https://github.com/PredictiveMovement/digital-twin/issues"
  },
  "homepage": "https://github.com/PredictiveMovement/digital-twin#readme",
  "dependencies": {
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "eslint": "^8.39.0",
    "eslint-plugin-rxjs": "^5.0.3",
    "lerna": "^6.6.2",
    "prettier": "^2.5.1"
  },
  "packageManager": "pnpm@10.17.0"
}
